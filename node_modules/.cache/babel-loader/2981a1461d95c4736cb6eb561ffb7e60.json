          :total=\"total\"\r\n          @current-change=\"loadList\"\r\n          prev-text=\"上一页\"\r\n          next-text=\"下一页\"\r\n        >\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script setup>\r\nimport { CaretRight } from \"@element-plus/icons-vue\";\r\n</script>\r\n<script>\r\nimport { getList } from \"@/api/protal\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      category: 2,\r\n      total: 0,\r\n      list: [],\r\n    };\r\n  },\r\n  methods: {\r\n    loadList(pageNum) {\r\n      getList({ pageNum: pageNum, pageSize: 10, category: this.category }).then((res) => {\r\n        this.list = res.data;\r\n        this.total = res.total;\r\n      });\r\n    },\r\n    clickCategory(category){\r\n      this.category = category;\r\n      this.loadList(1);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadList(1);\r\n  },\r\n  created(){\r\n    let category = this.$route.query.category;\r\n     if(!category||!this.$consts.activityCategorys.find(item =>item.id==category)){\r\n       category = this.$consts.activityCategorys[0].id;\r\n     }\r\n     this.category = category;\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\"  scoped>\r\n</style>"]}]}