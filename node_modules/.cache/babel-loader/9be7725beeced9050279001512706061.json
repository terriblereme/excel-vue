F,CAZD;MAaD,CA7BD;IA8BD;;EApCM,CARI;;EA8CbiB,OAAO,GAAE;IACJ,IAAId,EAAE,GAAG,KAAKe,MAAL,CAAYC,MAAZ,CAAmBhB,EAA5B;IACA,KAAKC,IAAL,CAAUD,EAAV;EACJ,CAjDY;;EAkDbiB,OAAO,GAAG,CAAE;;AAlDC,CAAf;AAJA,SAASC,UAAT,QAA2B,yBAA3B","names":["getDetail","getList","data","pre","next","methods","clickPreNext","id","load","$router","replace","then","res","pageNum","pageSize","category","endTime","createTime","total","startTime","sort","created","$route","params","mounted","ArrowRight"],"sources":["E:/excel/src/views/protal/recruitment-detail.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <img :src=\"require(`@/assets/img/ct${data.category}.png`)\" class=\"adv-top\" />\r\n    <div class=\"div-center container\">\r\n      <div class=\"breadcrumb\">\r\n        您当前位置：\r\n        <a @click=\"$router.push('/recruitment')\">人才招聘</a>\r\n        <el-icon><arrow-right /></el-icon>\r\n        <span>{{ data.title }}详情</span>\r\n      </div>\r\n      <div class=\"line\">\r\n        <hr />\r\n        <span data-aos=\"fade-down\"> DETAILS</span>\r\n        <hr />\r\n      </div>\r\n      <div class=\"detail\">\r\n        <h2 class=\"title\">{{ data.title }}</h2>\r\n        \r\n        <div v-html=\"data.content\"></div>\r\n      </div>\r\n      <div class=\"pn\">\r\n        <a\r\n          v-if=\"pre != null\"\r\n          @click=\"clickPreNext(pre.id)\"\r\n        >\r\n          上一篇:{{ pre.title.slice(0, 15) }}\r\n          <span v-if=\"pre.title.length > 15\">...</span>\r\n        </a>\r\n        <a\r\n          v-if=\"next != null\"\r\n          @click=\"clickPreNext(next.id)\"\r\n        >\r\n          下一篇:{{ next.title.slice(0, 15) }}\r\n          <span v-if=\"next.title.length > 15\">...</span>\r\n        </a>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script setup>\r\nimport { ArrowRight } from \"@element-plus/icons-vue\";\r\n</script>\r\n<script>\r\nimport { getDetail, getList } from \"@/api/protal\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      data: {},\r\n      pre: null,\r\n      next: null,\r\n    };\r\n  },\r\n  methods: {\r\n     clickPreNext(id){\r\n         this.load(id);\r\n         this.$router.replace(`/recruitment/detail/${id}`);\r\n     },\r\n    load(id) {\r\n      getDetail(id).then((res) => {\r\n        this.data = res.data;\r\n        //获取上一篇\r\n        getList({\r\n          pageNum: 1,\r\n          pageSize: 1,\r\n          category: this.data.category,\r\n          endTime: this.data.createTime,\r\n        }).then((res) => {\r\n          if (res.total > 0) {\r\n            this.pre = res.data[0];\r\n          }else{\r\n              this.pre=null;\r\n          }\r\n        });\r\n        //获取下一篇\r\n        getList({\r\n          pageNum: 1,\r\n          pageSize: 1,\r\n          category: this.data.category,\r\n          startTime: this.data.createTime,\r\n          sort: \"asc\",\r\n        }).then((res) => {\r\n          if (res.total > 0) {\r\n            this.next = res.data[0];\r\n          }else{\r\n              this.next=null;\r\n          }\r\n        });\r\n      });\r\n    },\r\n  },\r\n  created(){\r\n       let id = this.$route.params.id;\r\n       this.load(id);\r\n  },\r\n  mounted() {},\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\"  scoped>\r\n\r\n</style>"]}]}